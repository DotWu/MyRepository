<style>
    .ui-selected{
        background-color:blue;
        color:white;
    }
    .ui-menu:after{
        content:".";
        display:block;
        clear:both;
        visibility:hidden;
        line-height:0;
        height:0;
    }
    .ui-menu .ui-menu-item {
        display: inline-block;
        float: left;
        margin: 0px;
        padding: 10px;
        width: auto;
    }
        .ui-menu .ui-menu-item a {
            width: 200px;
            padding: 10px;
        }
    .selectt {
        width: auto;
        padding: 3px 2%;
        margin: 0;
        text-align: center;
        text-align-last: center;
    }
        .selectt option {
            text-align: center;
        }
</style>

