@using TE.NetCore.Controllers
@model HomePageViewModel
@{
    ViewBag.Title = "用户列表";
    Layout = "~/Views/Shared/_Layout.cshtml";
 }
<h1>用户列表</h1>
<div class="m-bottom-20">这个消息来自 Home 控制器下的 Index 的视图文件 index.cshtml </div>
<table>
    <tr>
        <td>ID</td>
        <td>姓名</td>
        <td>操作</td>
    </tr>
    @foreach (var sys_u in Model.sys_User) {

        <tr>
            <td>@sys_u.Id</td>
            <td>@sys_u.UserName</td>
            <td class="txt-left">
            <a asp-action="Detail" asp-route-Id="@sys_u.Id">详情</a>&nbsp;
                <a asp-controller="Home" asp-action="Edit" asp-route-Id="@sys_u.Id">编辑</a>
            </td>
        </tr>
     }
</table>
<p><a asp-action="Create">新增用户</a></p>
